<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:mapbox="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.sandilemazibuko.groovappbeta.MapsActivity"
    android:orientation="vertical"
    android:gravity="center_horizontal">

    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/home_buttons"
        >

        <com.mapbox.mapboxsdk.views.MapView
        android:id="@+id/mapview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        mapbox:access_token="pk.eyJ1IjoibXNlbnRyaSIsImEiOiJjaWdpOHoxMmg4NW1rdXFtMm14ZHN5aDdyIn0.MnZ_4gAK9h1zQLBwkeV1zA"></com.mapbox.mapboxsdk.views.MapView>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/home_buttons"
        >

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#000"

            >

            <TableRow

                android:gravity="center_horizontal">

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="fill_parent"
                    android:id="@+id/imageView7"
                    android:src="@drawable/first_home" />

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="fill_parent"
                    android:id="@+id/imageView8"
                    android:src="@drawable/send_home" />

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="fill_parent"
                    android:id="@+id/imgHome"
                    android:src="@drawable/main_home" />

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="fill_parent"
                    android:id="@+id/imageView10"
                    android:src="@drawable/third_home" />

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="fill_parent"
                    android:id="@+id/imageView11"
                    android:src="@drawable/fourth_home" />
            </TableRow>
        </TableLayout>
    </RelativeLayout>
    <!-- note the access token string created in the previous step -->
</RelativeLayout>
